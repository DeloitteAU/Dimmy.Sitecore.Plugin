<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<PackageTags>dimmyplugin</PackageTags>
		<AssemblyVersion>0.0.5.0</AssemblyVersion>
		<FileVersion>0.0.5.0</FileVersion>
		<PackageProjectUrl>dimmy.dev/plugin/sitecore</PackageProjectUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>0.0.5</Version>
		<Authors>Aaron Job</Authors>
		<Company>Deloitte Digital Asia Pacific</Company>
		<PackageVersion>0.0.1</PackageVersion>
		<Title>Dimmy.Sitecore.Plugin</Title>
		<Copyright>https://github.com/DeloitteDigitalAPAC/Dimmy.Sitecore.Plugin</Copyright>
		<PackageLicenseUrl>https://raw.githubusercontent.com/DeloitteDigitalAPAC/Dimmy.Sitecore.Plugin/master/LICENSE</PackageLicenseUrl>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="TopologyTemplates\docker-compose.xp.yml.template" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="TopologyTemplates\docker-compose.xm.template.yml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Properties\" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Dimmy.Cli" Version="0.0.2" />
	</ItemGroup>

</Project>